  "devDependencies": {
    "@babel/core": "~7.22",
    "@babel/preset-env": "~7.22",
    "babel-loader": "~9.1",
    "jest-environment-jsdom": "~29.5",
    "sass": "~1.63",
    "webpack": "~5.86",
    "webpack-cli": "~5.1"
  },
  "dependencies": {
    "jest": "~29.5",
