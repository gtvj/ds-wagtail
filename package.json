{
  "name": "ds-wagtail",
  "version": "6.3.0",
  "description": "## Local development",
  "private": true,
  "scripts": {
    "compile:css": "sass sass/etna.scss:templates/static/css/dist/etna.css",
    "compile:js": "webpack --config webpack.config.js --mode=production",
